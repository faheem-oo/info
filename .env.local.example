# Copy this to .env.local and fill in your values

# Airtable config (Optional - Primary if configured)
AIRTABLE_BASE_ID=
AIRTABLE_API_TOKEN=
# Name of the table in your base (e.g., Feedback or Table 1)
AIRTABLE_TABLE_NAME=Feedback

# Google Sheets config (Service Account)
# Get your Google Sheets ID from the URL: https://docs.google.com/spreadsheets/d/{GOOGLE_SHEETS_ID}/edit
GOOGLE_SHEETS_ID=
# Share the sheet with this service account email for write/read access
GOOGLE_SERVICE_ACCOUNT_EMAIL=
# Paste the private key (replace newlines with \n), e.g. -----BEGIN PRIVATE KEY-----\n...\n-----END PRIVATE KEY-----\n
GOOGLE_SERVICE_ACCOUNT_PRIVATE_KEY=
# Name of the sheet tab (e.g., Feedback)
GOOGLE_SHEET_NAME=Feedback

# Optional legacy placeholders
POSTGRES_URL=
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=
